{
    "manifest_version": 2,
  
    "name": "隐藏NBA球赛评分",
    "description": "隐藏NBA球赛评分，无负担看球赛",
    "version": "1.0",
  
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "browser_action": {
      "default_icon": {
        "19": "images/icon19.png",
        "38": "images/icon38.png"
      },
      "default_popup": "popup.html"
    }
  }